                                <div class="d-flex w-25"><p style="margin:auto">Ratings</p></div>
                                <div class=" text-warning gap-2">

                                    @{
                                        var ratingavg = @ViewBag.Avgratingview;

                                        if (ratingavg == 0)
                                        {
                                            <p style="color:black;">No rating found</p>

                                        }

                                        else
                                        {


